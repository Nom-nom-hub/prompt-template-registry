{
  "name": "prompt-registry",
  "version": "3.0.0",
  "description": "A library for retrieving and searching reusable prompt templates with remote synchronization",
  "type": "module",
  "main": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "test": "node test.mjs",
    "sync": "node --experimental-fetch index.mjs sync",
    "validate:prompt": "node validate-prompt.js",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "prompt",
    "registry",
    "templates",
    "ai",
    "ml",
    "sync",
    "remote"
  ],
  "author": "Prompt Template Registry Contributors",
  "license": "MIT",
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0"
  },
  "lint-staged": {
    "*.json": [
      "node validate-prompt.js",
      "git add"
    ],
    "*.{js,mjs}": [
      "node test.mjs"
    ]
  },
  "dependencies": {
    "ansi-escapes": "^7.0.0",
    "ansi-regex": "^6.2.0",
    "ansi-styles": "^6.2.1",
    "braces": "^3.0.3",
    "chalk": "^5.6.0",
    "cli-cursor": "^5.0.0",
    "cli-truncate": "^4.0.0",
    "colorette": "^2.0.20",
    "commander": "^13.1.0",
    "cross-spawn": "^7.0.6",
    "debug": "^4.4.1",
    "emoji-regex": "^10.5.0",
    "environment": "^1.1.0",
    "eventemitter3": "^5.0.1",
    "execa": "^8.0.1",
    "fill-range": "^7.1.1",
    "get-east-asian-width": "^1.3.1",
    "get-stream": "^8.0.1",
    "human-signals": "^5.0.0",
    "is-fullwidth-code-point": "^4.0.0",
    "is-number": "^7.0.0",
    "is-stream": "^3.0.0",
    "isexe": "^2.0.0",
    "lilconfig": "^3.1.3",
    "listr2": "^8.3.3",
    "log-update": "^6.1.0",
    "merge-stream": "^2.0.0",
    "micromatch": "^4.0.8",
    "mimic-fn": "^4.0.0",
    "mimic-function": "^5.0.1",
    "ms": "^2.1.3",
    "npm-run-path": "^5.3.0",
    "onetime": "^6.0.0",
    "path-key": "^3.1.1",
    "picomatch": "^2.3.1",
    "pidtree": "^0.6.0",
    "restore-cursor": "^5.1.0",
    "rfdc": "^1.4.1",
    "shebang-command": "^2.0.0",
    "shebang-regex": "^3.0.0",
    "signal-exit": "^4.1.0",
    "slice-ansi": "^5.0.0",
    "string-argv": "^0.3.2",
    "string-width": "^7.2.0",
    "strip-ansi": "^7.1.0",
    "strip-final-newline": "^3.0.0",
    "to-regex-range": "^5.0.1",
    "which": "^2.0.2",
    "wrap-ansi": "^9.0.0",
    "yaml": "^2.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nom-nom-hub/prompt-template-registry.git"
  },
  "bugs": {
    "url": "https://github.com/Nom-nom-hub/prompt-template-registry/issues"
  },
  "homepage": "https://github.com/Nom-nom-hub/prompt-template-registry#readme"
}
